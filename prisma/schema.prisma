generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model students {
  nama    String? @db.VarChar(50)
  alamat  String? @db.VarChar(100)
  nim     String  @id @db.VarChar(15)
  jurusan String? @db.VarChar(30)
}

model Faculty {
  id Int @id @default(autoincrement())
  name String
}